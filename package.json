{
  "name": "occamsrazor-validator",
  "version": "8.0.0",
  "description": "A duck-typing library",
  "main": "index.js",
  "scripts": {
    "test": "mocha tests/**/*.js",
    "watch": "npm run test -- -w",
    "lint": "./node_modules/.bin/eslint --ext .js ./src ./tests",
    "release:major": "./node_modules/.bin/npm-release major",
    "release:minor": "./node_modules/.bin/npm-release minor",
    "release:patch": "./node_modules/.bin/npm-release patch",
    "precommit": "npm run lint",
    "prepush": "npm run test"
  },
  "keywords": [
    "duck typing",
    "plugins",
    "components",
    "registry",
    "adapters"
  ],
  "repository": "git@github.com:sithmel/occamsrazor-validator.git",
  "author": "Maurizio Lupo <maurizio.lupo@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "chai": "^1.10.0",
    "eslint": "^1.10.3",
    "husky": "^0.10.2",
    "mocha": "^2.1.0",
    "npm-release": "^1.0.0"
  },
  "dependencies": {
    "occamsrazor-match": "^2.2.1"
  }
}
